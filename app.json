{
  "name": "pylight.org",
  "scripts": {
    "postdeploy": "sh -c 'python pywaw/manage.py syncdb --noinput; python pywaw/manage.py migrate --noinput;'"
  },
  "env": {
    "DJANGO_SETTINGS_MODULE": {
      "required": true
    },
    "WEB_CONCURRENCY": {
      "required": true
    }
  },
  "formation": {},
  "addons": [
    "heroku-postgresql"
  ],
  "buildpacks": [
    {
      "url": "heroku/python"
    }
  ]
}